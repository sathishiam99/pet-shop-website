    <ellipse cx="0" cy="-40" rx="60" ry="20" fill="#FFFFFF" opacity="0.9" />
    
    <!-- Food in Bowl -->
    <ellipse cx="0" cy="-40" rx="40" ry="15" fill="#FFD166" opacity="0.8" />
    
