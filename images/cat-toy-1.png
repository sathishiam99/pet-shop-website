<svg width="300" height="300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="300" height="300" fill="#f8f9fa" rx="10" ry="10"/>
  
  <!-- Cat Toy - Interactive Wand -->
  <g transform="translate(150, 120)">
    <!-- Wand Handle -->
    <rect x="-100" y="-10" width="80" height="20" rx="5" ry="5" fill="#06d6a0" stroke="#1d3557" stroke-width="2"/>
    
    <!-- Wand String -->
    <path d="M-20,0 C0,-20 20,-30 40,-20 C60,-10 80,10 100,0" stroke="#1d3557" stroke-width="2" fill="none" stroke-dasharray="5,5"/>
    
    <!-- Feather Toy -->
    <g transform="translate(100, 0)">
      <!-- Base -->
      <circle cx="0" cy="0" r="10" fill="#ef476f" stroke="#1d3557" stroke-width="1"/>
      
      <!-- Feathers -->
      <path d="M0,-10 C10,-30 20,-40 30,-35 C20,-25 10,-15 0,-10 Z" fill="#ffd166" stroke="#1d3557" stroke-width="1"/>
      <path d="M5,-8 C25,-20 35,-25 40,-15 C30,-10 15,-5 5,-8 Z" fill="#06d6a0" stroke="#1d3557" stroke-width="1"/>
      <path d="M8,-5 C30,-10 40,-5 42,5 C30,5 15,0 8,-5 Z" fill="#ef476f" stroke="#1d3557" stroke-width="1"/>
      <path d="M8,5 C25,15 35,25 30,30 C20,25 10,15 8,5 Z" fill="#3a86ff" stroke="#1d3557" stroke-width="1"/>
      <path d="M0,10 C5,30 0,40 -10,40 C-10,30 -5,20 0,10 Z" fill="#ffd166" stroke="#1d3557" stroke-width="1"/>
      <path d="M-8,5 C-25,15 -35,25 -30,30 C-20,25 -10,15 -8,5 Z" fill="#06d6a0" stroke="#1d3557" stroke-width="1"/>
      <path d="M-8,-5 C-30,-10 -40,-5 -42,5 C-30,5 -15,0 -8,-5 Z" fill="#ef476f" stroke="#1d3557" stroke-width="1"/>
    </g>
    
    <!-- Bell -->
    <circle cx="100" cy="0" r="5" fill="#ffd166" stroke="#1d3557" stroke-width="1"/>
    <path d="M97,3 L103,3" stroke="#1d3557" stroke-width="1"/>
    <circle cx="100" cy="2" r="1" fill="#1d3557"/>
  </g>
  
  <!-- Cat Silhouette Playing -->
  <g transform="translate(80, 180)">
    <path d="M0,0 C5,-5 15,-5 20,0 C25,5 30,5 35,0 L40,5 L45,-5 C50,-10 55,-10 60,-5 L65,-15 L70,-5 C75,0 80,0 85,-5 C90,-10 95,-10 100,-5 C105,0 110,5 100,10 C90,15 80,20 70,20 C60,20 50,15 45,10 C40,15 35,20 25,20 C15,20 5,15 0,10 C-5,5 -5,5 0,0 Z" fill="#1d3557" opacity="0.2"/>
  </g>
  
  <!-- Product Details -->
  <g transform="translate(20, 260)">
    <text x="0" y="0" font-family="Arial" font-size="14" fill="#1d3557" font-weight="bold">Interactive Feather Wand</text>
    <text x="0" y="20" font-family="Arial" font-size="12" fill="#1d3557">For All Cat Ages</text>
  </g>
  
  <!-- Price Tag -->
  <g transform="translate(220, 270)">
    <rect x="0" y="0" width="60" height="25" rx="12.5" ry="12.5" fill="#ef476f"/>
    <text x="30" y="17" font-family="Arial" font-size="14" text-anchor="middle" fill="#ffffff" font-weight="bold">$9.99</text>
  </g>
  
  <!-- Product Highlights -->
  <g transform="translate(20, 230)">
    <circle cx="10" cy="0" r="8" fill="#06d6a0"/>
    <text x="25" y="4" font-family="Arial" font-size="10" fill="#1d3557">Interactive</text>
    
    <circle cx="100" cy="0" r="8" fill="#06d6a0"/>
    <text x="115" y="4" font-family="Arial" font-size="10" fill="#1d3557">Exercise</text>
    
    <circle cx="180" cy="0" r="8" fill="#06d6a0"/>
    <text x="195" y="4" font-family="Arial" font-size="10" fill="#1d3557">Replaceable</text>
  </g>
  
  <!-- Brand Tag -->
  <g transform="translate(20, 30)">
    <rect x="0" y="0" width="80" height="25" rx="5" ry="5" fill="#ef476f"/>
    <text x="40" y="17" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff" font-weight="bold">PAWFECT</text>
  </g>
</svg>