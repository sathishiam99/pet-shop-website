<svg xmlns="http://www.w3.org/2000/svg" width="400" height="250" viewBox="0 0 400 250">
  <!-- Background -->
  <rect width="400" height="250" rx="10" ry="10" fill="#F8F9FA" />
  
  <!-- Image Area -->
  <rect x="20" y="20" width="360" height="160" rx="8" ry="8" fill="#FFD166" />
  
  <!-- Cat Silhouette -->
  <g transform="translate(200, 100) scale(0.8)">
    <!-- Cat Body -->
    <ellipse cx="0" cy="0" rx="70" ry="35" fill="#FFFFFF" opacity="0.9" />
    
    <!-- Cat Head -->
    <circle cx="-50" cy="-20" r="35" fill="#FFFFFF" opacity="0.9" />
    
    <!-- Cat Ears -->
    <polygon points="-70,-40 -80,-70 -50,-50" fill="#FFFFFF" opacity="0.9" />
    <polygon points="-30,-40 -20,-70 -50,-50" fill="#FFFFFF" opacity="0.9" />
    
    <!-- Cat Tail -->
    <path d="M70,0 Q100,-40 120,-10" stroke="#FFFFFF" stroke-width="10" stroke-linecap="round" fill="none" opacity="0.9" />
    
    <!-- Cat Legs -->
    <rect x="-40" y="25" width="8" height="25" fill="#FFFFFF" opacity="0.9" />
    <rect x="-15" y="25" width="8" height="25" fill="#FFFFFF" opacity="0.9" />
    <rect x="15" y="25" width="8" height="25" fill="#FFFFFF" opacity="0.9" />
    <rect x="40" y="25" width="8" height="25" fill="#FFFFFF" opacity="0.9" />
  </g>
  
  <!-- Title Area -->
  <rect x="20" y="190" width="360" height="40" rx="5" ry="5" fill="#FFFFFF" />
  
  <!-- Title Text -->
  <text x="200" y="215" font-family="Nunito, sans-serif" font-size="16" font-weight="700" fill="#2A363B" text-anchor="middle">Understanding Your Cat's Body Language</text>
  
  <!-- Category Tag -->
  <g transform="translate(50, 40)">
    <rect x="-40" y="-15" width="80" height="30" rx="15" ry="15" fill="#4ECDC4" />
    <text x="0" y="5" font-family="Nunito, sans-serif" font-size="12" font-weight="700" fill="#FFFFFF" text-anchor="middle">BEHAVIOR</text>
  </g>
  
  <!-- Date -->
  <text x="350" y="40" font-family="Poppins, sans-serif" font-size="12" font-weight="400" fill="#FFFFFF" text-anchor="end">July 3, 2023</text>
</svg>