<svg xmlns="http://www.w3.org/2000/svg" width="600" height="300" viewBox="0 0 600 300">
  <!-- Background -->
  <rect width="600" height="300" rx="20" ry="20" fill="#4ECDC4" />
  
  <!-- Decorative Elements -->
  <circle cx="50" cy="50" r="30" fill="#FFD166" opacity="0.6" />
  <circle cx="550" cy="250" r="40" fill="#FF6B6B" opacity="0.6" />
  
  <!-- New Tag -->
  <g transform="translate(450, 70)">
    <polygon points="0,0 -20,-20 -80,-20 -80,20 -20,20" fill="#FF6B6B" />
    <text x="-50" y="5" font-family="Nunito, sans-serif" font-size="16" font-weight="800" fill="#FFFFFF" text-anchor="middle">NEW</text>
  </g>
  
  <!-- Toy Box -->
  <g transform="translate(150, 180)">
    <!-- Box -->
    <rect x="-70" y="-60" width="140" height="120" rx="10" ry="10" fill="#FFFFFF" />
    <rect x="-70" y="-60" width="140" height="30" rx="10" ry="10" fill="#FFD166" />
    
    <!-- Toys sticking out -->
    <circle cx="-40" cy="-70" r="15" fill="#FF6B6B" />
    <rect x="-5" y="-90" width="10" height="30" fill="#4ECDC4" />
    <polygon points="20,-70 40,-90 60,-70" fill="#FFD166" />
    
    <!-- Label -->
    <rect x="-30" y="-10" width="60" height="20" rx="5" ry="5" fill="#FF6B6B" />
    <text x="0" y="5" font-family="Nunito, sans-serif" font-size="12" font-weight="700" fill="#FFFFFF" text-anchor="middle">TOYS</text>
  </g>
  
  <!-- Cat Playing -->
  <g transform="translate(350, 200)">
    <!-- Cat Body -->
    <ellipse cx="0" cy="0" rx="50" ry="30" fill="#FFFFFF" />
    
    <!-- Cat Head -->
    <circle cx="-40" cy="-20" r="30" fill="#FFFFFF" />
    
    <!-- Cat Ears -->
    <polygon points="-55,-40 -65,-55 -45,-45" fill="#FFFFFF" />
    <polygon points="-25,-40 -15,-55 -35,-45" fill="#FFFFFF" />
    
    <!-- Cat Face -->
    <circle cx="-50" cy="-25" r="3" fill="#2A363B" />
    <circle cx="-30" cy="-25" r="3" fill="#2A363B" />
    <path d="M-45,-15 L-35,-15" stroke="#2A363B" stroke-width="2" />
    <path d="M-55,-30 L-45,-35" stroke="#2A363B" stroke-width="1" />
    <path d="M-55,-20 L-45,-25" stroke="#2A363B" stroke-width="1" />
    <path d="M-25,-30 L-35,-35" stroke="#2A363B" stroke-width="1" />
    <path d="M-25,-20 L-35,-25" stroke="#2A363B" stroke-width="1" />
    
    <!-- Cat Tail -->
    <path d="M50,0 Q70,-30 90,-10" stroke="#FFFFFF" stroke-width="10" stroke-linecap="round" fill="none" />
  </g>
  
  <!-- Text -->
  <text x="300" y="80" font-family="Nunito, sans-serif" font-size="40" font-weight="800" fill="#FFFFFF" text-anchor="middle" stroke="#2A363B" stroke-width="1">New Arrivals</text>
  <text x="300" y="120" font-family="Poppins, sans-serif" font-size="20" font-weight="600" fill="#2A363B" text-anchor="middle">Check out our latest pet toys collection</text>
  
  <!-- Button -->
  <rect x="250" y="140" width="100" height="40" rx="20" ry="20" fill="#FF6B6B" />
  <text x="300" y="165" font-family="Nunito, sans-serif" font-size="16" font-weight="700" fill="#FFFFFF" text-anchor="middle">Discover</text>
</svg>