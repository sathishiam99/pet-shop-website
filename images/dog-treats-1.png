<svg width="300" height="300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="300" height="300" fill="#f8f9fa" rx="10" ry="10"/>
  
  <!-- Treat Container -->
  <g transform="translate(75, 50)">
    <!-- Container Body -->
    <rect x="0" y="0" width="150" height="180" rx="10" ry="10" fill="#ffd166" stroke="#1d3557" stroke-width="2"/>
    
    <!-- Container Lid -->
    <rect x="10" y="10" width="130" height="30" rx="5" ry="5" fill="#ffffff" stroke="#1d3557" stroke-width="1"/>
    
    <!-- Brand Logo -->
    <circle cx="75" cy="25" r="15" fill="#ef476f" stroke="#1d3557" stroke-width="1"/>
    <text x="75" y="30" font-family="Arial" font-size="10" text-anchor="middle" fill="#ffffff" font-weight="bold">PAWFECT</text>
    
    <!-- Product Name -->
    <rect x="25" y="60" width="100" height="40" rx="5" ry="5" fill="#ffffff"/>
    <text x="75" y="80" font-family="Arial" font-size="10" text-anchor="middle" fill="#1d3557" font-weight="bold">NATURAL</text>
    <text x="75" y="92" font-family="Arial" font-size="10" text-anchor="middle" fill="#1d3557" font-weight="bold">DOG TREATS</text>
    
    <!-- Treats Illustration -->
    <g transform="translate(30, 110)">
      <!-- Bone Shape -->
      <path d="M0,10 C0,5 5,0 10,0 C15,0 20,5 20,10 C20,15 25,20 30,20 C35,20 40,15 40,10 C40,5 45,0 50,0 C55,0 60,5 60,10 C60,15 55,20 50,20 C45,20 40,25 40,30 C40,35 35,40 30,40 C25,40 20,35 20,30 C20,25 15,20 10,20 C5,20 0,15 0,10 Z" fill="#f4a261" stroke="#1d3557" stroke-width="1"/>
      
      <!-- Peanut Shape -->
      <ellipse cx="80" cy="15" rx="15" ry="10" fill="#e9c46a" stroke="#1d3557" stroke-width="1" transform="rotate(30, 80, 15)"/>
      <ellipse cx="90" cy="25" rx="15" ry="10" fill="#e9c46a" stroke="#1d3557" stroke-width="1" transform="rotate(-30, 90, 25)"/>
    </g>
    
    <!-- Weight -->
    <text x="75" y="170" font-family="Arial" font-size="14" text-anchor="middle" fill="#1d3557" font-weight="bold">8 OZ</text>
  </g>
  
  <!-- Product Details -->
  <g transform="translate(20, 260)">
    <text x="0" y="0" font-family="Arial" font-size="14" fill="#1d3557" font-weight="bold">Natural Peanut Butter Treats</text>
    <text x="0" y="20" font-family="Arial" font-size="12" fill="#1d3557">All Natural Ingredients</text>
  </g>
  
  <!-- Price Tag -->
  <g transform="translate(220, 270)">
    <rect x="0" y="0" width="60" height="25" rx="12.5" ry="12.5" fill="#ef476f"/>
    <text x="30" y="17" font-family="Arial" font-size="14" text-anchor="middle" fill="#ffffff" font-weight="bold">$12.99</text>
  </g>
  
  <!-- Product Highlights -->
  <g transform="translate(20, 230)">
    <circle cx="10" cy="0" r="8" fill="#06d6a0"/>
    <text x="25" y="4" font-family="Arial" font-size="10" fill="#1d3557">Grain-Free</text>
    
    <circle cx="100" cy="0" r="8" fill="#06d6a0"/>
    <text x="115" y="4" font-family="Arial" font-size="10" fill="#1d3557">No Additives</text>
    
    <circle cx="180" cy="0" r="8" fill="#06d6a0"/>
    <text x="195" y="4" font-family="Arial" font-size="10" fill="#1d3557">USA Made</text>
  </g>
</svg>