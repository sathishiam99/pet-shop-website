<svg width="300" height="300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="300" height="300" fill="#f8f9fa" rx="10" ry="10"/>
  
  <!-- Dog Collar -->
  <g transform="translate(150, 150)">
    <!-- Collar Circle -->
    <ellipse cx="0" cy="0" rx="100" ry="60" fill="none" stroke="#3a86ff" stroke-width="20" stroke-linecap="round" stroke-dasharray="0 180" transform="rotate(45, 0, 0)"/>
    <ellipse cx="0" cy="0" rx="100" ry="60" fill="none" stroke="#3a86ff" stroke-width="20" stroke-linecap="round" stroke-dasharray="0 180" transform="rotate(225, 0, 0)"/>
    
    <!-- Collar Buckle -->
    <g transform="translate(70, -40) rotate(45)">
      <rect x="-15" y="-10" width="30" height="20" rx="2" ry="2" fill="#ffd166" stroke="#1d3557" stroke-width="1"/>
      <rect x="-10" y="-5" width="20" height="10" rx="1" ry="1" fill="#1d3557"/>
      <circle cx="-5" cy="0" r="2" fill="#ffffff"/>
    </g>
    
    <!-- Collar Tag -->
    <g transform="translate(-70, 40) rotate(-45)">
      <circle cx="0" cy="0" r="15" fill="#ef476f" stroke="#1d3557" stroke-width="1"/>
      <text x="0" y="5" font-family="Arial" font-size="10" text-anchor="middle" fill="#ffffff" font-weight="bold">P</text>
    </g>
    
    <!-- Collar Pattern -->
    <g transform="rotate(45)">
      <rect x="-90" y="-5" width="10" height="10" fill="#ffffff" rx="2" ry="2"/>
      <rect x="-70" y="-5" width="10" height="10" fill="#ffffff" rx="2" ry="2"/>
      <rect x="-50" y="-5" width="10" height="10" fill="#ffffff" rx="2" ry="2"/>
      <rect x="-30" y="-5" width="10" height="10" fill="#ffffff" rx="2" ry="2"/>
      <rect x="-10" y="-5" width="10" height="10" fill="#ffffff" rx="2" ry="2"/>
      <rect x="10" y="-5" width="10" height="10" fill="#ffffff" rx="2" ry="2"/>
      <rect x="30" y="-5" width="10" height="10" fill="#ffffff" rx="2" ry="2"/>
      <rect x="50" y="-5" width="10" height="10" fill="#ffffff" rx="2" ry="2"/>
      <rect x="70" y="-5" width="10" height="10" fill="#ffffff" rx="2" ry="2"/>
    </g>
  </g>
  
  <!-- Product Details -->
  <g transform="translate(20, 260)">
    <text x="0" y="0" font-family="Arial" font-size="14" fill="#1d3557" font-weight="bold">Durable Nylon Dog Collar</text>
    <text x="0" y="20" font-family="Arial" font-size="12" fill="#1d3557">Medium Size (15-20")</text>
  </g>
  
  <!-- Price Tag -->
  <g transform="translate(220, 270)">
    <rect x="0" y="0" width="60" height="25" rx="12.5" ry="12.5" fill="#ef476f"/>
    <text x="30" y="17" font-family="Arial" font-size="14" text-anchor="middle" fill="#ffffff" font-weight="bold">$18.99</text>
  </g>
  
  <!-- Product Highlights -->
  <g transform="translate(20, 230)">
    <circle cx="10" cy="0" r="8" fill="#06d6a0"/>
    <text x="25" y="4" font-family="Arial" font-size="10" fill="#1d3557">Reflective</text>
    
    <circle cx="100" cy="0" r="8" fill="#06d6a0"/>
    <text x="115" y="4" font-family="Arial" font-size="10" fill="#1d3557">Waterproof</text>
    
    <circle cx="180" cy="0" r="8" fill="#06d6a0"/>
    <text x="195" y="4" font-family="Arial" font-size="10" fill="#1d3557">Adjustable</text>
  </g>
  
  <!-- Brand Tag -->
  <g transform="translate(20, 30)">
    <rect x="0" y="0" width="80" height="25" rx="5" ry="5" fill="#3a86ff"/>
    <text x="40" y="17" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff" font-weight="bold">PAWFECT</text>
  </g>
  
  <!-- Size Options -->
  <g transform="translate(220, 30)">
    <rect x="0" y="0" width="25" height="25" rx="5" ry="5" fill="#f8f9fa" stroke="#1d3557" stroke-width="1"/>
    <text x="12.5" y="17" font-family="Arial" font-size="12" text-anchor="middle" fill="#1d3557">S</text>
    
    <rect x="30" y="0" width="25" height="25" rx="5" ry="5" fill="#3a86ff" stroke="#1d3557" stroke-width="1"/>
    <text x="42.5" y="17" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff">M</text>
    
    <rect x="60" y="0" width="25" height="25" rx="5" ry="5" fill="#f8f9fa" stroke="#1d3557" stroke-width="1"/>
    <text x="72.5" y="17" font-family="Arial" font-size="12" text-anchor="middle" fill="#1d3557">L</text>
  </g>
</svg>