<svg width="300" height="300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="300" height="300" fill="#f8f9fa" rx="10" ry="10"/>
  
  <!-- Cat Litter Box -->
  <g transform="translate(150, 150)">
    <!-- Box Body -->
    <rect x="-80" y="-60" width="160" height="120" rx="10" ry="10" fill="#3a86ff" stroke="#1d3557" stroke-width="2"/>
    
    <!-- Box Top Edge -->
    <rect x="-80" y="-60" width="160" height="20" rx="10" ry="10" fill="#1d3557" opacity="0.1"/>
    
    <!-- Litter Surface -->
    <rect x="-70" y="-40" width="140" height="90" rx="5" ry="5" fill="#f4f1de" stroke="#1d3557" stroke-width="1"/>
    
    <!-- Litter Texture -->
    <g fill="#e9c46a" opacity="0.5">
      <circle cx="-50" cy="-20" r="3"/>
      <circle cx="-30" cy="-25" r="2"/>
      <circle cx="-10" cy="-15" r="4"/>
      <circle cx="10" cy="-25" r="3"/>
      <circle cx="30" cy="-20" r="2"/>
      <circle cx="50" cy="-15" r="3"/>
      
      <circle cx="-60" cy="0" r="2"/>
      <circle cx="-40" cy="5" r="3"/>
      <circle cx="-20" cy="-5" r="2"/>
      <circle cx="0" cy="0" r="4"/>
      <circle cx="20" cy="5" r="3"/>
      <circle cx="40" cy="-5" r="2"/>
      <circle cx="60" cy="0" r="3"/>
      
      <circle cx="-50" cy="20" r="4"/>
      <circle cx="-30" cy="25" r="2"/>
      <circle cx="-10" cy="15" r="3"/>
      <circle cx="10" cy="25" r="2"/>
      <circle cx="30" cy="20" r="4"/>
      <circle cx="50" cy="15" r="3"/>
    </g>
    
    <!-- Paw Prints -->
    <g fill="#1d3557" opacity="0.2">
      <!-- Paw 1 -->
      <circle cx="-40" cy="10" r="3"/>
      <circle cx="-45" cy="5" r="2"/>
      <circle cx="-35" cy="5" r="2"/>
      <ellipse cx="-40" cy="15" rx="4" ry="3"/>
      
      <!-- Paw 2 -->
      <circle cx="0" cy="-10" r="3"/>
      <circle cx="-5" cy="-15" r="2"/>
      <circle cx="5" cy="-15" r="2"/>
      <ellipse cx="0" cy="-5" rx="4" ry="3"/>
      
      <!-- Paw 3 -->
      <circle cx="40" cy="10" r="3"/>
      <circle cx="35" cy="5" r="2"/>
      <circle cx="45" cy="5" r="2"/>
      <ellipse cx="40" cy="15" rx="4" ry="3"/>
    </g>
  </g>
  
  <!-- Product Label -->
  <g transform="translate(150, 70)">
    <rect x="-60" y="-20" width="120" height="40" rx="5" ry="5" fill="#ffffff" stroke="#1d3557" stroke-width="1"/>
    
    <!-- Brand Logo -->
    <text x="0" y="-5" font-family="Arial" font-size="12" text-anchor="middle" fill="#1d3557" font-weight="bold">PAWFECT</text>
    
    <!-- Product Name -->
    <text x="0" y="15" font-family="Arial" font-size="10" text-anchor="middle" fill="#1d3557" font-weight="bold">CLUMPING CAT LITTER</text>
  </g>
  
  <!-- Product Details -->
  <g transform="translate(20, 260)">
    <text x="0" y="0" font-family="Arial" font-size="14" fill="#1d3557" font-weight="bold">Premium Clumping Litter</text>
    <text x="0" y="20" font-family="Arial" font-size="12" fill="#1d3557">Odor Control Formula</text>
  </g>
  
  <!-- Price Tag -->
  <g transform="translate(220, 270)">
    <rect x="0" y="0" width="60" height="25" rx="12.5" ry="12.5" fill="#ef476f"/>
    <text x="30" y="17" font-family="Arial" font-size="14" text-anchor="middle" fill="#ffffff" font-weight="bold">$19.99</text>
  </g>
  
  <!-- Product Highlights -->
  <g transform="translate(20, 230)">
    <circle cx="10" cy="0" r="8" fill="#06d6a0"/>
    <text x="25" y="4" font-family="Arial" font-size="10" fill="#1d3557">99% Dust-Free</text>
    
    <circle cx="100" cy="0" r="8" fill="#06d6a0"/>
    <text x="115" y="4" font-family="Arial" font-size="10" fill="#1d3557">Odor Control</text>
    
    <circle cx="180" cy="0" r="8" fill="#06d6a0"/>
    <text x="195" y="4" font-family="Arial" font-size="10" fill="#1d3557">20 lbs</text>
  </g>
  
  <!-- Weight Indicator -->
  <g transform="translate(40, 40)">
    <rect x="-20" y="-15" width="40" height="30" rx="5" ry="5" fill="#ffd166" stroke="#1d3557" stroke-width="1"/>
    <text x="0" y="5" font-family="Arial" font-size="10" text-anchor="middle" fill="#1d3557" font-weight="bold">20 LBS</text>
  </g>
</svg>