<svg xmlns="http://www.w3.org/2000/svg" width="600" height="300" viewBox="0 0 600 300">
  <!-- Background -->
  <rect width="600" height="300" rx="20" ry="20" fill="#FF6B6B" />
  
  <!-- Decorative Elements -->
  <circle cx="50" cy="250" r="30" fill="#4ECDC4" opacity="0.6" />
  <circle cx="550" cy="50" r="40" fill="#FFD166" opacity="0.6" />
  
  <!-- Shipping Box -->
  <g transform="translate(150, 180)">
    <!-- Box -->
    <rect x="-60" y="-50" width="120" height="100" fill="#FFD166" />
    <rect x="-60" y="-50" width="120" height="20" fill="#2A363B" />
    
    <!-- Box Flaps -->
    <path d="M-60,-50 L-80,-70 L40,-70 L60,-50" fill="#FFD166" />
    <path d="M60,-50 L40,-70 L40,-90 L60,-70 Z" fill="#FFD166" />
    <path d="M-60,-50 L-80,-70 L-80,-90 L-60,-70 Z" fill="#FFD166" />
    
    <!-- Shipping Label -->
    <rect x="-40" y="-20" width="80" height="40" fill="#FFFFFF" />
    <rect x="-30" y="-10" width="60" height="5" fill="#2A363B" opacity="0.3" />
    <rect x="-30" y="0" width="40" height="5" fill="#2A363B" opacity="0.3" />
    <rect x="-30" y="10" width="50" height="5" fill="#2A363B" opacity="0.3" />
  </g>
  
  <!-- Delivery Truck -->
  <g transform="translate(400, 180)">
    <!-- Truck Body -->
    <rect x="-70" y="-40" width="140" height="70" rx="10" ry="10" fill="#FFFFFF" />
    <rect x="-100" y="-10" width="30" height="40" rx="5" ry="5" fill="#FFFFFF" />
    <rect x="-100" y="-30" width="30" height="20" rx="5" ry="5" fill="#4ECDC4" />
    
    <!-- Wheels -->
    <circle cx="-60" cy="40" r="15" fill="#2A363B" />
    <circle cx="-60" cy="40" r="8" fill="#FFFFFF" />
    <circle cx="50" cy="40" r="15" fill="#2A363B" />
    <circle cx="50" cy="40" r="8" fill="#FFFFFF" />
    
    <!-- Details -->
    <rect x="-50" y="-20" width="100" height="30" fill="#4ECDC4" />
    <path d="M-50,-20 L50,-20 L60,-50 L-60,-50 Z" fill="#4ECDC4" />
    <text x="0" y="0" font-family="Nunito, sans-serif" font-size="12" font-weight="700" fill="#FFFFFF" text-anchor="middle">FREE</text>
  </g>
  
  <!-- Paw Icon -->
  <g transform="translate(300, 70) scale(0.15)">
    <path fill="#FFFFFF" d="M256 224c-79.5 0-144 64.5-144 144s64.5 144 144 144 144-64.5 144-144-64.5-144-144-144zm59.4 180.2c-5.5 12.2-15.6 22-28.1 27.3-12.5 5.3-26.3 5.3-38.8 0-12.5-5.3-22.6-15.1-28.1-27.3-1.2-2.7-.2-5.9 2.4-7.2 2.7-1.2 5.9-.2 7.2 2.4 4.4 9.7 12.4 17.5 22.5 21.8 9.8 4.2 21 4.2 30.8 0 10.1-4.3 18.1-12.1 22.5-21.8 1.2-2.7 4.4-3.7 7.2-2.4 2.7 1.3 3.6 4.5 2.4 7.2z"/>
  </g>
  
  <!-- Text -->
  <text x="300" y="80" font-family="Nunito, sans-serif" font-size="40" font-weight="800" fill="#FFFFFF" text-anchor="middle" stroke="#2A363B" stroke-width="1">Free Shipping</text>
  <text x="300" y="120" font-family="Poppins, sans-serif" font-size="20" font-weight="600" fill="#FFFFFF" text-anchor="middle">On all orders over $50</text>
  
  <!-- Button -->
  <rect x="250" y="140" width="100" height="40" rx="20" ry="20" fill="#FFD166" />
  <text x="300" y="165" font-family="Nunito, sans-serif" font-size="16" font-weight="700" fill="#2A363B" text-anchor="middle">Shop Now</text>
</svg>