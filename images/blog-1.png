<svg xmlns="http://www.w3.org/2000/svg" width="400" height="250" viewBox="0 0 400 250">
  <!-- Background -->
  <rect width="400" height="250" rx="10" ry="10" fill="#F8F9FA" />
  
  <!-- Image Area -->
  <rect x="20" y="20" width="360" height="160" rx="8" ry="8" fill="#4ECDC4" />
  
  <!-- Dog Silhouette -->
  <g transform="translate(200, 100) scale(0.8)">
    <!-- Dog Body -->
    <ellipse cx="0" cy="0" rx="80" ry="40" fill="#FFFFFF" opacity="0.9" />
    
    <!-- Dog Head -->
    <circle cx="-60" cy="-20" r="40" fill="#FFFFFF" opacity="0.9" />
    
    <!-- Dog Ears -->
    <path d="M-90,-40 Q-110,-60 -100,-20 Z" fill="#FFFFFF" opacity="0.9" />
    <path d="M-30,-40 Q-10,-60 -20,-20 Z" fill="#FFFFFF" opacity="0.9" />
    
    <!-- Dog Tail -->
    <path d="M80,0 Q100,-30 120,0" stroke="#FFFFFF" stroke-width="10" stroke-linecap="round" fill="none" opacity="0.9" />
    
    <!-- Dog Legs -->
    <rect x="-40" y="30" width="10" height="30" fill="#FFFFFF" opacity="0.9" />
    <rect x="-10" y="30" width="10" height="30" fill="#FFFFFF" opacity="0.9" />
    <rect x="20" y="30" width="10" height="30" fill="#FFFFFF" opacity="0.9" />
    <rect x="50" y="30" width="10" height="30" fill="#FFFFFF" opacity="0.9" />
  </g>
  
  <!-- Title Area -->
  <rect x="20" y="190" width="360" height="40" rx="5" ry="5" fill="#FFFFFF" />
  
  <!-- Title Text -->
  <text x="200" y="215" font-family="Nunito, sans-serif" font-size="16" font-weight="700" fill="#2A363B" text-anchor="middle">10 Essential Tips for New Dog Owners</text>
  
  <!-- Category Tag -->
  <g transform="translate(50, 40)">
    <rect x="-30" y="-15" width="60" height="30" rx="15" ry="15" fill="#FF6B6B" />
    <text x="0" y="5" font-family="Nunito, sans-serif" font-size="12" font-weight="700" fill="#FFFFFF" text-anchor="middle">TIPS</text>
  </g>
  
  <!-- Date -->
  <text x="350" y="40" font-family="Poppins, sans-serif" font-size="12" font-weight="400" fill="#FFFFFF" text-anchor="end">June 15, 2023</text>
</svg>